package com.example.fragment;

public class BookContent {
	public static class Book{
		public Integer id;
		public String title;
		public String desc;
		public Book(Integer id,String title,String desc){
			this.id=id;
			this.title=title;
			this.desc=desc;
		}
		@Override
		public String toString(){
			return title;
		}
	}
}
