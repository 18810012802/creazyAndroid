	/**
	 * 杩炴帴绔栫嚎
	 * @return
	 */
	public String getHLineStr(DeviceInfo dev){
		double x = dev.getX();
		double y = dev.getY();
		String len = String.valueOf(dev.getLen());
		String name = dev.getName();
//		String color = dev.getColor();//濡傛灉涓�绉嶈澶囩殑棰滆壊涓嶅彉鍙互鐩存帴鍐欐锛屼笉鐢╣et鑾峰彇
		String str = "<path fill=\"#027871\" d=\"M16,0c-0.55,0-1,0.45-1,1v30c0,0.55,0.45,1,1,1s1-0.45,1-1V1C17,0.45,16.55,0,16,0z\"" +
				" transform=\"matrix(1,0,0,"+len+","+x+","+y+")\" xmlns=\"http://www.w3.org/2000/svg\" />\n";
//				"<text x=\"180\" y=\"180\" stroke=\"rgb(0,0,0)\" transform=\"matrix(1,0,0,1,"+x+","+y+")\" font-size=\"14.0\" font-family=\"榛戜綋\" fill=\"rgb(0,0,0)\" >\n" +
//				""+name+"</text>";
		return str;
	}
	/**
	 * 杩炴帴妯嚎
	 */
	public String getVLineStr(DeviceInfo dev){
		double x = dev.getX();
		double y = dev.getY();
		String len = String.valueOf(dev.getLen());
