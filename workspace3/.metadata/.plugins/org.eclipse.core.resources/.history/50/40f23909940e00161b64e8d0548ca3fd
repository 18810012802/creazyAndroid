	/**
	 * 鍖呭惈test
	 * @return
	 */
	public String getBusStr(DeviceInfo di){
		String busCode="<path fill='"+di.getColor()+"' d='M29.039,13h-26c-1.657,0-3,1.343-3,3s1.343,3,3,3h26c1.657,0,3-1.343,3-3S30.696,13,29.039,13z' transform='matrix(8.439996,0,0,0.9999999,"+di.getX()+","+di.getY()+")' xmlns='http://www.w3.org/2000/svg' />\r\t";
		String text="<text x='"+(di.getX()-20)+"' y='"+di.getY()+"' xml:space='preserve' font-family='榛戜綋' font-size='14' fill='#000000' stroke='#000000' font-weight='bold' baseline-shift='baseline'>"+
				di.getName()+"</text>\r\t";
		return busCode+text;
	}
	
