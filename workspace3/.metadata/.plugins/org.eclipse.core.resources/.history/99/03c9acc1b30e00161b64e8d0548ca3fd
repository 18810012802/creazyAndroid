package com.jb.genemap.main;

/**
 * 设备svg图形参数信息
 * @author yjl
 *
 */
public class DeviceInfo {
	private double x;//横坐标值
	private double y;//纵坐标值
	private String devType;//设备类型
	private String name;//设备名称
	private String id ;
	public int getBusConnCount() {
		return busConnCount;
	}
	public void setBusConnCount(int busConnCount) {
		this.busConnCount = busConnCount;
	}
	private String color;//svg图形颜色
	private double len;//线长度
	private double thicker;//线厚度
	private double width;//设备宽度
	private double height;//设备高度
	private DeviceInfo dev;//存储父节点
	private int busConnCount =0;
	
	public DeviceInfo getDev() {
		return dev;
	}
	public void setDev(DeviceInfo dev) {
		this.dev = dev;
	}
	public double getX() {
		return x;
	}
	public void setX(double x) {
		this.x = x;
	}
	public double getY() {
		return y;
	}
	public void setY(double y) {
		this.y = y;
	}
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getDevType() {
		return devType;
	}
	public void setDevType(String devType) {
		this.devType = devType;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getColor() {
		return color;
	}
	public void setColor(String color) {
		this.color = color;
	}
	public double getLen() {
		return len;
	}
	public void setLen(double len) {
		this.len = len;
	}
	public double getThicker() {
		return thicker;
	}
	public void setThicker(double thicker) {
		this.thicker = thicker;
	}
	public double getWidth() {
		return width;
	}
	public void setWidth(double width) {
		this.width = width;
	}
	public double getHeight() {
		return height;
	}
	public void setHeight(double height) {
		this.height = height;
	}
}
