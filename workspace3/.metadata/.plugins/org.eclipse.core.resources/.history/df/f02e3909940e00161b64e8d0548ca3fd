package svg.jb.genemap.main;

/**
 * 璁惧svg鍥惧舰鍙傛暟淇℃伅
 * @author yjl
 *
 */
public class DeviceInfo {
	private double x;//妯潗鏍囧��
	private double y;//绾靛潗鏍囧��
	private String devType;//璁惧绫诲瀷
	private String name;//璁惧鍚嶇О
	private String id ;
	public int getBusConnCount() {
		return busConnCount;
	}
	public void setBusConnCount(int busConnCount) {
		this.busConnCount = busConnCount;
	}
	private String color;//svg鍥惧舰棰滆壊
	private double len;//绾块暱搴�
	private double thicker;//绾垮帤搴�
	private double width;//璁惧瀹藉害
	private double height;//璁惧楂樺害
	private DeviceInfo dev;//瀛樺偍鐖惰妭鐐�
	private int busConnCount =0;
	
	public DeviceInfo getDev() {
		return dev;
	}
	public void setDev(DeviceInfo dev) {
		this.dev = dev;
	}
	public double getX() {
		return x;
	}
	public void setX(double x) {
		this.x = x;
	}
	public double getY() {
		return y;
	}
	public void setY(double y) {
		this.y = y;
	}
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public String getDevType() {
		return devType;
	}
	public void setDevType(String devType) {
		this.devType = devType;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getColor() {
		return color;
	}
	public void setColor(String color) {
		this.color = color;
	}
	public double getLen() {
		return len;
	}
	public void setLen(double len) {
		this.len = len;
	}
	public double getThicker() {
		return thicker;
	}
	public void setThicker(double thicker) {
		this.thicker = thicker;
	}
	public double getWidth() {
		return width;
	}
	public void setWidth(double width) {
		this.width = width;
	}
	public double getHeight() {
		return height;
	}
	public void setHeight(double height) {
		this.height = height;
	}
}
